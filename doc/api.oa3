        - processor
        - memory
        - storage
        - requests
        - accounts
      properties:
        timestamp:
          type: integer
          format: int32
        processor:
          type: integer
          format: int32
        memory:
          type: integer
          format: int64
        storage:
          type: integer
          format: int64
        requests:
          type: integer
          format: int64
        accounts:
          type: integer
          format: int64

    Service:
      type: object
      required:
