  /admin/accounts/{amigoId}/alerts/{alertId}:
    delete:
      tags:
        - admin
      description: Remove alert from target account
      operationId: remove-server-alert
      parameters:
        - name: token
          in: query
          description: installation token for admin access
          required: true
          schema:
            type: string
            format: secure-token
        - name: amigoId
          in: path
          description: id of account entry
          required: true
          schema:
            type: string
            format: amigo-id
        - name: alertId
          in: path
          description: id of alert entry
          required: true
          schema:
            type: string
      responses:
        '200':
          description: successful operation
        '403':
          description: access denied
        '404':
          description: not found

components:
  schemas:

    ServerInfo:
